{
  "name": "reactjs-js-tsdk-template",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://settld-lab.github.io/battle_royale_client",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "deploy:dev": "gh-pages -d dist -r git@github.com:alphagamer7/miniapp.git -b gh-pages",
    "deploy:prod": "gh-pages -d dist -r git@github.com:settld-lab/battle_royale_client.git -b gh-pages",
    "build:dev": "cross-env VITE_ENV=development vite build --base=/miniapp/",
    "build:prod": "cross-env VITE_ENV=production vite build --base=/battle_royale_client/",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "predeploy": "npm run build"
  },
  "dependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "@tailwindcss/vite": "^4.0.1",
    "@telegram-apps/telegram-ui": "^2.1.4",
    "@tonconnect/ui-react": "^2.0.5",
    "@twa-dev/sdk": "^7.10.1",
    "@twa-dev/types": "^7.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "bs58": "^6.0.0",
    "buffer": "^6.0.3",
    "eruda": "^3.0.1",
    "expo-linking": "^7.0.5",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.474.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.24.0",
    "tailwindcss": "^4.0.1",
    "tweetnacl": "^1.0.3",
    "vite-plugin-node-polyfills": "^0.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.1.1",
    "globals": "^15.2.0",
    "vite": "^5.1.5"
  }
}
